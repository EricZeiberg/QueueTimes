<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ExpandableLayout">
        <attr name="el_headerLayout" format="reference"/>
        <attr name="el_contentLayout" format="reference" />
        <attr name="el_duration" format="integer" />
    </declare-styleable>
    <declare-styleable name="PullRefreshLayout">
        <attr name="type" format="enum">
            <enum name="material" value="0" />
            <enum name="circles" value="1" />
            <enum name="water_drop" value="2" />
            <enum name="ring" value="3" />
        </attr>
        <attr name="colors" format="reference"/>
    </declare-styleable>
</resources>